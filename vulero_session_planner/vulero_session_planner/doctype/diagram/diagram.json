{
  "doctype": "DocType",
  "name": "Diagram",
  "module": "Vulero Session Planner",
  "custom": 0,
  "autoname": "DIAG-.#####",
  "title_field": "title",
  "fields": [
    {
      "fieldname": "diagram_section",
      "fieldtype": "Section Break",
      "label": "Diagram"
    },
    {
      "fieldname": "title",
      "fieldtype": "Data",
      "label": "Title",
      "in_list_view": 1
    },
    {
      "fieldname": "pitch_type",
      "fieldtype": "Select",
      "label": "Pitch Type",
